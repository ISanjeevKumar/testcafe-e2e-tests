{
  "name": "testcafe-e2e-tests",
  "version": "1.0.0",
  "description": "testcafe e2e tests ",
  "main": "index.js",
  "scripts": {
    "test": "ENVIRONMENT=local testcafe test-iphone && testcafe test-chrome && testcafe test-firefox",
    "iphone:run:local": "ENVIRONMENT=local testcafe chrome:emulation:device=ipad pro tests/smoke",
    "chrome:debug:local": "ENVIRONMENT=local testcafe chrome tests/smoke --debug-on-fail",
    "chrome:run:local": "ENVIRONMENT=local node scripts/testcafe.js -b chrome -t 'tests/smoke' xunit:result.xml",
    "chrome:run": "ENVIRONMENT=prod node scripts/testcafe.js -b chrome -t 'tests/smoke' xunit:result.xml",
    "chrome:run:headless": "node scripts/testcafe.js -b chrome:headless -t 'tests/smoke' xunit:result.xml"
  },
  "author": "Sanjeev Kumar",
  "license": "ISC",
  "dependencies": {
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "testcafe-reporter-xunit": "^2.1.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "testcafe": "^1.17.1"
  }
}
